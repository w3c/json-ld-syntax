@prefix geojson: <https://purl.org/geojson/vocab#>.
[
  a geojson:Feature ;
  geojson:bbox (-1.0e1 -1.0e1 1.0e1 1.0e1) ;
  geojson:geometry [
    a geojson:Polygon ;
    geojson:coordinates (
      (
        (-1.0e1 -1.0e1)
        (1.0e1 -1.0e1)
        (1.0e1 1.0e1)
        (-1.0e1 -1.0e1)
      )
    )
  ]
] .